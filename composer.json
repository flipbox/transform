{
  "name": "flipboxdigital/transform",
  "description": "Transform complex data structures into simple arrays",
  "version": "1.0.0",
  "keywords": [
    "transform",
    "flipbox",
    "serialize"
  ],
  "require": {
    "php": ">=7.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0",
    "squizlabs/php_codesniffer": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Flipbox\\Transform\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Flipbox\\Transform\\Tests\\": "tests/src/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packagist.org"
    }
  ]
}